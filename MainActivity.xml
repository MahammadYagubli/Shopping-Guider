    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mahammad.shopping_guider.MainActivity"


   android:background="#daad86" >

    <include
        layout="@layout/appmain"
        android:id="@+id/app"

        > </include>


    <Button
